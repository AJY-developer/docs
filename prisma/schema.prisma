generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Chapter {
  id      Int     @id @default(autoincrement())
  chapter String
  content String?
  topic   String
  urlname String

  @@unique([urlname, topic])
  @@unique([chapter, topic])
  @@index([urlname, topic])
}
